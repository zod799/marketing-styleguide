{
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "impliedStrict": true
        }
    },
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    },
    "globals": {
        "console": false,
        "Clipboard": false
    },
    "rules": {
        "no-console": 1
    }
}